  <!-- FlowRelationship -->
  <xsd:element name="flowRelationship" type="FlowRelationship" substitutionGroup="core:modelElement" />
  <xsd:complexType name="FlowRelationship">
    <xsd:complexContent>
      <xsd:extension base="core:ModelElement">
