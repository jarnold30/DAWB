  <xsd:complexType name="PostConditionMoniker">
    <xsd:annotation>
      <xsd:documentation>Moniker for PostCondition instance.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ConditionMoniker">
