    <xsd:complexContent>
      <xsd:extension base="FlowElementMoniker">
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  
  <!-- MergeBranch -->
  <xsd:element name="mergeBranch" type="MergeBranch" substitutionGroup="flowElement" />
  <xsd:complexType name="MergeBranch">
